{
	"name": "vscode-decoration-performance-test",
	"displayName": "vscode-decoration-performance-test",
	"description": "vscode-decoration-performance-test",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:vscode-decoration-performance-test.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-decoration-performance-test.helloWorld",
