		"lint": "eslint source --ext ts"
	},
	"devDependencies": {
		"@types/vscode": "^1.52.0",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"typescript": "^4.1.2"
	}
